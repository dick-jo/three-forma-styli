{
  "name": "@three-forma-styli/cli",
  "version": "0.1.4",
  "type": "module",
  "description": "CLI tool for generating design tokens from TypeScript definitions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "tfs": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch 'src/**' --ext 'ts' --exec 'ts-node src/index.ts'",
    "start": "node dist/index.js"
  },
  "keywords": ["design-tokens", "cli", "css-variables"],
  "author": "three",
  "license": "ISC",
  "dependencies": {
    "@inquirer/prompts": "^7.2.0",
    "@three-forma-styli/core": "^0.1.0",
    "@three-forma-styli/themes": "^0.1.0",
    "chalk": "4.1.2",
    "commander": "^13.1.0",
    "esbuild": "^0.24.0",
    "fs-extra": "^11.3.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.9",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
